\tab printf("para c = 5+10%%2/2, c = %d\\n\\n", c);\par
\tab printf("para d = -(3%%5)*3/2, d = %d \\n\\n", d);\par
\par
\par
\tab system("PAUSE");\par
\tab return 0;\par
\}\par
