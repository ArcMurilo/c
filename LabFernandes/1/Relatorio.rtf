\lang1033 float f;            /* floating point type                  */\lang1046\par
\lang1033 double g;           /* double precision floating point      */\lang1046\par
\pard\lang1033\par
\pard\li360\lang1046 Teste com\par
   a = 1023; b = 2222; c = 123;  d = 1234;  e = 'X';\par
   f = 3.14159;  g = 3.1415926535898;\par
\pard\fi-360\li360\sl360\slmult1 #include <stdio.h>\par
#include <stdlib.h>\par
\par
int main() \{\par
\tab int a;\par
\tab long int b;\par
\tab short int c;\par
\tab unsigned int d;\par
\tab char e;\par
\tab float f;\par
\tab double g;\par
\par
\tab printf("Insira uma letra: ");\par
